{
  "name": "user-management-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/petermunachi/user-management-app.git",
  "scripts": {
    "dev": "prisma generate && next dev",
    "postinstall": "npx prisma migrate deploy && npx prisma generate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pre-build": "tsc --noEmit",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "migration:generate": "npx prisma migrate dev --name",
    "migration:run": "npx prisma migrate dev",
    "database:schema:generate": "npx prisma generate"
  },
  "engines": {
    "node": "18.15.0",
    "npm": "9.5.0"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@reduxjs/toolkit": "^1.9.3",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.4",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "next-redux-wrapper": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.7",
    "react-loader-spinner": "^5.3.4",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.2",
    "redux-thunk": "^2.4.2",
    "styled-components": "^5.3.9",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-styled-components": "^2.0.7",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "prisma": "^4.11.0"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
